<AnchorPane prefHeight="400.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Views.modules.Admin.ArticlesGes">

    <GridPane alignment="CENTER" hgap="10.0" prefHeight="707.0" prefWidth="1232.0" vgap="10.0">
